<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane maxHeight="900.0" maxWidth="1200.0" minHeight="500.0" minWidth="550.0" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pomtimer.Controller">
   <top>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="pomodoroButton" mnemonicParsing="false" onAction="#pressPomodoroButton" text="Pomodoro">
               <HBox.margin>
                  <Insets right="30.0" />
               </HBox.margin></Button>
            <Button fx:id="shortBreakButton" mnemonicParsing="false" onAction="#pressShortBreakButton" text="Short Break">
               <HBox.margin>
                  <Insets left="30.0" right="30.0" />
               </HBox.margin></Button>
            <Button fx:id="longBreakButton" mnemonicParsing="false" onAction="#pressLongBreakButton" text="Long Break">
               <HBox.margin>
                  <Insets left="30.0" />
               </HBox.margin></Button>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets></HBox>
   </top>
   <center>
      <VBox alignment="CENTER" prefHeight="382.0" prefWidth="800.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="timerLabel" text="Timer">
               <VBox.margin>
                  <Insets bottom="20.0" top="20.0" />
               </VBox.margin>
               <font>
                  <Font size="74.0" />
               </font></Label>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="startButton" mnemonicParsing="false" onAction="#pressStartButton" text="Start">
                     <HBox.margin>
                        <Insets right="25.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="stopButton" mnemonicParsing="false" onAction="#pressStopButton" text="Stop">
                     <HBox.margin>
                        <Insets left="25.0" right="25.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="resetButton" mnemonicParsing="false" onAction="#pressResetButton" text="Reset">
                     <HBox.margin>
                        <Insets left="25.0" right="25.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="skipButton" mnemonicParsing="false" onAction="#pressSkipButton" text="Skip">
                     <HBox.margin>
                        <Insets left="25.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <Label fx:id="pomodoroNumLabel" text="Pomodoro: #" />
            <Label text="Working on: " />
         </children>
      </VBox>
   </center>
   <bottom>
      <HBox alignment="CENTER" prefHeight="176.0" prefWidth="822.0" spacing="380.0">
         <children>
            <Pane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <TextField fx:id="pomTaskText" layoutX="15.0" layoutY="14.0" prefHeight="42.0" prefWidth="337.0" promptText="What are you working on?" />
                  <Spinner fx:id="pomSpinner" editable="true" layoutX="130.0" layoutY="74.0" prefHeight="33.0" prefWidth="77.0" />
                  <Label layoutX="21.0" layoutY="82.0" text="Est Pomodoros" />
                  <Button layoutX="270.0" layoutY="117.0" mnemonicParsing="false" onAction="#pressAddTaskButton" text="Add Task" />
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin></Pane>
            <VBox prefHeight="176.0" prefWidth="680.0">
               <children>
                  <ListView fx:id="taskList" prefHeight="200.0" prefWidth="200.0" />
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Button mnemonicParsing="false" text="Delete">
                           <HBox.margin>
                              <Insets right="10.0" />
                           </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" text="Edit Name">
                           <HBox.margin>
                              <Insets left="10.0" right="10.0" />
                           </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" text="Edit # Pomodoros">
                           <HBox.margin>
                              <Insets left="10.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </HBox>
   </bottom>
</BorderPane>
